@model WalkDrum.Models.WalkInInfo
<div class="form-horizontal">
        <h4 class="sessiontitle">VENUE</h4>
        
        
        <div class="form-group">
            <h4 class="itemlabels">VENUE</h4>
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.venue, new { Class = "form-control" })
            </div>
        </div>

        <div class="form-group">
            <h4 class="itemlabels">CONTACT PERSON</h4>
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.contactperson, new { Class = "form-control" })
            </div>
        </div>
    </div>
