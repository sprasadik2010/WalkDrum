@model WalkDrum.Models.WalkInInfo
@Html.AntiForgeryToken()

<div class="form-horizontal">
    <h4 class="sessiontitle">COMPANY INFO</h4>
    
    
    <div class="form-group">
        <h4 class="itemlabels">COMPANY NAME</h4>
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.companyinfo.CompanyName, new { Class = "form-control" })
            @*<input type="text" class="form-control" id="CompanyName" />*@
        </div>
    </div>

    <div class="form-group">
        <h4 class="itemlabels">E-MAIL</h4>
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.companyinfo.Email, new { Class = "form-control" })
            @*<input type="text" class="form-control" id="Email" />*@
        </div>
    </div>

    <div class="form-group">
        <h4 class="itemlabels">PHONE</h4>
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.companyinfo.Phone, new { Class = "form-control" })
            @*<input type="text" class="form-control" id="Phone" />*@
        </div>
    </div>
</div>
